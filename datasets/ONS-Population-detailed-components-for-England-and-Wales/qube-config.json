{
    "$schema": "https://purl.org/csv-cubed/qube-config/v1",
    "id": "ons-population-estimates-for-uk-england-and-wales",
    "title": "Population detailed components for England and Wales",
    "published": "2022-08-01",
    "columns": {
        "Period": {
            "from_template": "year"
        },
        "Local Authority Code": {
            "type": "dimension",
            "label": "Geography",
            "code_list": false,
            "cell_uri_template": "http://statistics.data.gov.uk/id/statistical-geography/{+local_authority_code}",
            "from_existing": "http://purl.org/linked-data/sdmx/2009/dimension#refArea"
        },
        "Age": {
            "type": "dimension",
            "code_list": "./age_code-list.json"
        },
        "Sex": {
            "type": "dimension",
            "code_list": "./sex_code-list.json"
        },
        "Measure Type": {
            "type":"measures"            
        },
        "Value": {
            "type": "observations",
            "data_type": "integer",
            "unit": {
                "label": "Number",
                "from_existing": "https://qudt.org/vocab/unit/NUM"
            }
        }
    },
    "sizingNotes": "",
    "notes": ""
}